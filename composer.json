{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "aminyazdanpanah/php-ffmpeg-video-streaming": "^1.2.18",
        "aws/aws-sdk-php-laravel": "^3.9",
        "cerdic/css-tidy": "^2.2.1",
        "guzzlehttp/guzzle": "^7.9.2",
        "intervention/image": "^3.11.2",
        "intervention/image-laravel": "^1.5.5",
        "laravel/framework": "^12",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.0",
        "marvinlabs/laravel-discord-logger": "^1.4.3",
        "nette/caching": "^3.3.1",
        "nette/database": "^3.2.6",
        "nette/utils": "^4.0",
        "nullpunkt/lz-string-php": "^1.3",
        "owen-oj/laravel-getid3": "^2.4",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "pusher/pusher-php-server": "^7.2.7",
        "ratchet/pawl": "^0.4.1",
        "react/async": "^4 || ^3 || ^2",
        "symfony/html-sanitizer": "^7.2.3",
        "symfony/yaml": "^6.4",
        "tinymce/tinymce": "^7.7.1",
        "tymon/jwt-auth": "^2.2.0",
        "voku/anti-xss": "^4.1",
        "yajra/laravel-datatables": "12.0"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.24.1",
        "laravel/pint": "^v1.20.0",
        "laravel/sail": "^v1.40.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Lib\\": "app/lib/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "ApiClients\\Client\\Pusher\\": "vendor/pusher-client"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "reautoloadClass": [
            "composer dump-autoload",
            "echo Autoload Done"
        ],
        "cleanAll": [
            "@php artisan optimize:clear",
            "echo clean All Done"
        ],
        "queuework": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan queue:work --timeout=300"
        ],
        "schedulework": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan schedule:work2"
        ],
        "runServer": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan serve"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
